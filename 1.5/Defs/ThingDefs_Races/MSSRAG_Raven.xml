<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<ThingDef ParentName="BigBirdThingBase">
    <defName>MSSRAG_Huginn</defName>
    <label>Huginn</label>
    <description>A large flightless bird with beady eyes on its ugly face. With its bad attitude, it is the jerk of the natural world. Bother it, and it is guaranteed to seek revenge.</description>
    <statBases>
      <MoveSpeed>5.5</MoveSpeed>
      <MarketValue>200</MarketValue>
      <ComfyTemperatureMin>-8</ComfyTemperatureMin>
      <ComfyTemperatureMax>50</ComfyTemperatureMax>
      <FilthRate>8</FilthRate>
    </statBases>
    <race>
      <herdAnimal>false</herdAnimal>
      <baseBodySize>0.6</baseBodySize>
      <baseHealthScale>0.9</baseHealthScale>
      <baseHungerRate>0.28</baseHungerRate>
      <foodType>VegetarianRoughAnimal</foodType>
      <wildness>0.95</wildness>
      <roamMtbDays>3</roamMtbDays>
      <lifeExpectancy>45</lifeExpectancy>
      <useMeatFrom>Cassowary</useMeatFrom>
      <manhunterOnTameFailChance>1.00</manhunterOnTameFailChance>
      <manhunterOnDamageChance>1.00</manhunterOnDamageChance>
      <lifeStageAges>
        <li>
          <def>AnimalBabyTiny</def>
          <minAge>0</minAge>
          <soundWounded>Pawn_Chick_Call</soundWounded>
          <soundDeath>Pawn_Chick_Call</soundDeath>
          <soundCall>Pawn_Chick_Call</soundCall>
          <soundAngry>Pawn_Chick_Call</soundAngry>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.1</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.2222</minAge>
        </li>
      </lifeStageAges>
      <headPosPerRotation>
        <li>(0.0, 0, 0.385)</li>
        <li>(0.29, 0, 0.31)</li>
        <li>(0.0, 0, 0.27)</li>
        <li>(-0.29, 0, 0.31)</li>
      </headPosPerRotation>
    </race>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>EggHuginnFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>3.33</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>1</eggCountRange>
      </li>
    </comps>
    <tools>
      <li>
        <label>claws</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>beak</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>6</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <tradeTags>
      <li>AnimalUncommon</li>
    </tradeTags>
  </ThingDef>

